arxivId = {NIHMS150003},
author = {Inoue, Manabu and Mlynash, Michael and Straka, Matus and Kemp, Stephanie and Jovin, Tudor G. and Tipirneni, Aaryani and Hamilton, Scott A. and Marks, Michael P. and Bammer, Roland and Lansberg, Maarten G. and Albers, Gregory W.},
doi = {10.1161/STROKEAHA.111.000371},
eprint = {NIHMS150003},
isbn = {0039-2499},
issn = {00392499},
journal = {Stroke},
keywords = {acute stroke,endovascular treatment,magnetic resonance imaging,perfusion-weighted imaging,reperfusion},
month = {jul},
number = {7},
pages = {1885--1890},
pmid = {23704106},
title = {{Clinical outcomes strongly associated with the degree of reperfusion achieved in target mismatch patients: Pooled data from the diffusion and perfusion imaging evaluation for understanding stroke evolution studies}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23704106 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3810454 http://stroke.ahajournals.org/cgi/doi/10.1161/STROKEAHA.111.000371},
